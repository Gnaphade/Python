    "def fib_gen():\n",
    "    a=0\n",
    "    b=1\n",
    "    while True:\n",
    "        c=a\n",
    "        a=b\n",
    "        b=c+a\n",
    "        yield c\n",
    "        \n",
    "f=fib_gen()\n",
    "#print(next(f))\n",
    "for i in range (20):\n",
    "    print(next(f),end=\" \")"
